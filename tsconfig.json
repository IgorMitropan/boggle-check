{
  "compilerOptions": {
    "declaration": true,
    "lib": ["ES6", "dom"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES6",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}